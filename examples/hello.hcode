let x = 5;
let y = 10;

if (x < y) {
    print("x is less than y");
} else {
    print("x is greater than or equal to y");
}

let sum = 0;
let i = 0;

while (i < 5) {
    sum = sum + i;
    i = i + 1;
}

print("Sum from 0 to 4 is:");
print(sum);

for (let j = 0; j < 3; j = j + 1) {
    print("Loop iteration:");
    print(j);
}
